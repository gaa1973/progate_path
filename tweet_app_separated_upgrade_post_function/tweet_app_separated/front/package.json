{
  "name": "front",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:test": "FRONT_PORT=${FRONT_PORT:-3001} API_PORT=${API_PORT:-8001} NODE_ENV=test vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
    "format:fix": "prettier --write './**/*.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "express-validator": "^7.2.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0"
  }
}

{
  "name": "front",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "start:test": "npm run start",
    "build": "npm run build:css && npm run build:vite",
    "build:vite": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
    "format:fix": "prettier --write './**/*.{ts,tsx,js,jsx}'",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "vite": "^6.1.0"
  }
}

{
  "name": "e2e",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:add_a_number_page": "npm run test -- add_a_number_page.ts",
    "test:add_a_product_page": "npm run test -- add_a_product_page.ts",
    "test:route_parameter": "npm run test -- route_parameter.ts",
    "test:query_parameter": "npm run test -- query_parameter.ts",
    "test:layout_for_all_pages": "npm run test -- layout_for_all_pages.ts",
    "test:layout_for_specific_pages": "npm run test -- layout_for_specific_pages.ts",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
    "format:fix": "prettier --write './**/*.{ts,tsx,js,jsx}'",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-dev-server": "^11.0.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^24.2.1",
    "ts-jest": "^29.2.5"
  }
}
